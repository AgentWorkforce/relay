{
  "name": "@relay/dashboard-server",
  "version": "0.1.0",
  "description": "Relay dashboard server - HTTP/WebSocket server for agent coordination",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": ["dist", "README.md"],
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@relay/protocol": "0.1.0",
    "@relay/config": "0.1.0",
    "@relay/storage": "0.1.0",
    "@relay/bridge": "0.1.0",
    "@relay/utils": "0.1.0",
    "@relay/resiliency": "0.1.0",
    "@relay/trajectory": "0.1.0",
    "@relay/cloud": "0.1.0",
    "@relay/daemon": "0.1.0",
    "@relay/user-directory": "0.1.0",
    "@agent-relay/wrapper": "0.1.0",
    "express": "^4.21.2",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@types/express": "^5.0.1",
    "@types/node": "^22.19.3",
    "@types/ws": "^8.18.1",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4"
  },
  "publishConfig": {
    "access": "public"
  }
}
