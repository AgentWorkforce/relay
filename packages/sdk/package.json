{
  "name": "@agent-relay/sdk",
  "version": "0.1.0",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "bin",
    "README.md",
    "package.json"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json && npm run bundle:binary",
    "bundle:binary": "node ./scripts/bundle-agent-relay.mjs",
    "check": "tsc -p tsconfig.json --noEmit",
    "test:integration": "node --test dist/__tests__/integration.test.js",
    "example": "node dist/examples/example.js",
    "dashboard": "node dist/examples/dashboard.js",
    "prepack": "npm run build"
  },
  "devDependencies": {
    "@types/node": "^22.13.10",
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "@relaycast/sdk": "^0.2.1"
  }
}
